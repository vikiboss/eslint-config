{
  "name": "eslint-config",
  "version": "0.6.0",
  "private": true,
  "packageManager": "pnpm@8.6.11",
  "description": "Viki's shareable opinionated eslint config monorepo",
  "author": "Viki <hi@viki.moe> (https://github.com/vikiboss)",
  "repository": "git@github:vikiboss/eslint-config.git",
  "license": "MIT",
  "scripts": {
    "lint": "eslint --ext=.{js,jsx,ts,tsx,vue} .",
    "lint:fix": "eslint --ext=.{js,jsx,ts,tsx,vue} . --fix",
    "release": "bumpp package.json packages/*/package.json -y && pnpm -r publish"
  },
  "devDependencies": {
    "@types/eslint": "^8.44.2",
    "@types/node": "^18.17.5",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "@vmoe/eslint-config": "workspace:*",
    "@vmoe/prettier-config": "latest",
    "bumpp": "^9.1.1",
    "eslint": "^8.47.0",
    "prettier": "^3.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.1.6"
  }
}
