{
  "name": "eslint-config",
  "version": "0.3.0",
  "private": true,
  "packageManager": "pnpm@7.30.0",
  "description": "Viki's shareable eslint config monorepo",
  "author": "Viki <hi@viki.moe> (https://github.com/vikiboss)",
  "license": "MIT",
  "scripts": {
    "lint": "eslint --ext .{js,jsx,ts,tsx,vue} .",
    "lint:fix": "eslint --ext .{js,jsx,ts,tsx,vue} . --fix",
    "release": "bumpp package.json packages/*/package.json -y && pnpm -r publish"
  },
  "devDependencies": {
    "@types/eslint": "^8.37.0",
    "@types/node": "^18.15.11",
    "@types/react": "^18.0.31",
    "@types/react-dom": "^18.0.11",
    "@vmoe/eslint-config": "workspace:*",
    "@vmoe/prettier-config": "^1.0.0",
    "bumpp": "^9.1.0",
    "eslint": "^8.37.0",
    "prettier": "^2.8.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.0.3",
    "vue": "^3.2.47"
  }
}
